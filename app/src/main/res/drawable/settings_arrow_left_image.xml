
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="24dp"
    android:height="24dp"
    android:viewportWidth="24"
    android:viewportHeight="24">

    <!-- Arrow Body/Shaft -->
    <path
        android:strokeColor="#FFFFFF"
    android:strokeWidth="2"
    android:strokeLineCap="round"
    android:pathData="M20,12 L7,12" />
    <!--
        M20,12: Move to the right side (start of the shaft)
        L7,12:  Draw a line to the left, stopping before the arrowhead
    -->

    <!-- Arrow Head - Line 1 (Top part of chevron) -->
    <path
        android:strokeColor="@color/bright_text"
    android:strokeWidth="2"
    android:strokeLineCap="round"
    android:strokeLineJoin="round"
    android:pathData="M10,7 L5,12" />
    <!--
        M10,7: Move to the top point of the arrowhead
        L5,12: Draw a line down and to the left, to the tip of the arrow (center Y)
               The X-coordinate (5) is further left than the shaft's end (7)
               to give the arrowhead some width.
    -->

    <!-- Arrow Head - Line 2 (Bottom part of chevron) -->
    <path
        android:strokeColor="@color/bright_text"
    android:strokeWidth="2"
    android:strokeLineCap="round"
    android:strokeLineJoin="round"
    android:pathData="M10,17 L5,12" />
    <!--
        M10,17: Move to the bottom point of the arrowhead
        L5,12: Draw a line up and to the left, to the same tip of the arrow (center Y)
    -->
</vector>

